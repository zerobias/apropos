{
  "description": "Strong typed functional 'Either'",
  "name": "apropos",
  "version": "0.1.0",
  "license": "MIT",
  "main": "lib/index.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "types": "index.d.ts",
  "typings": "index.d.ts",
  "scripts": {
    "prepublish": "npm run rebuild:es && npm run rebuild:cjs",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run build:cjs && npm run build:es",
    "build:cjs": "cross-env BABEL_ENV=commonjs babel src/ -d lib -s",
    "build:es": "babel src/ -d es -s",
    "clean:cjs": "rimraf lib/",
    "clean:es": "rimraf es/",
    "rebuild:cjs": "npm run clean:cjs & npm run build:cjs",
    "rebuild:es": "npm run clean:es & npm run build:es"
  },
  "author": "Zero Bias",
  "devDependencies": {
    "@types/jest": "^20.0.6",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-block-scoping": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-plugin-transform-flow-comments": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "cross-env": "^5.0.5",
    "eslint": "^4.4.1",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-flowtype": "^2.35.0",
    "eslint-plugin-jest": "^20.0.3",
    "eslint-plugin-jsdoc": "^3.1.2",
    "flow-bin": "^0.52.0",
    "jest": "^20.0.4",
    "rimraf": "^2.6.1",
    "tslint": "^5.6.0",
    "typescript": "^2.4.2"
  },
  "keywords": [
    "either",
    "functional",
    "fp",
    "monad",
    "adt",
    "validation"
  ],
  "files": [
    "src",
    "lib",
    "es",
    ".flowconfig",
    "index.d.ts"
  ]
}
