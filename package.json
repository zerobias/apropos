{
  "description": "Strong typed functional 'Either', 'Maybe', 'Identity' and 'Tuple'",
  "name": "apropos-dev",
  "private": true,
  "version": "1.0.1",
  "license": "MIT",
  "scripts": {
    "types": "flow-copy-source src lib && flow-copy-source src es",
    "flow": "pm2 start flow-apropos.sh",
    "flow:stop": "pm2 stop flow-apropos",
    "test": "jest --config=jest.json",
    "build": "npm run build:cjs && npm run build:es",
    "build:cjs": "cross-env BABEL_ENV=commonjs babel src/ -d lib -s",
    "build:es": "babel src/ -d es -s",
    "clean": "npm run clean:es && npm run clean:cjs",
    "clean:cjs": "rimraf lib/",
    "clean:es": "rimraf es/",
    "rebuild:cjs": "npm run clean:cjs & npm run build:cjs",
    "rebuild:es": "npm run clean:es & npm run build:es"
  },
  "author": "Zero Bias",
  "devDependencies": {
    "@types/jest": "^21.1.8",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "cross-env": "^5.1.1",
    "documentation": "^5.3.3",
    "eslint": "^4.13.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-flowtype": "^2.39.1",
    "eslint-plugin-jest": "^21.4.2",
    "eslint-plugin-jsdoc": "^3.2.0",
    "flow-bin": "^0.61.0",
    "flow-copy-source": "^1.2.1",
    "flow-typed": "^2.2.3",
    "jest": "^21.2.1",
    "lerna": "^2.5.1",
    "pm2": "^2.8.0",
    "ramda": "^0.25.0",
    "rimraf": "^2.6.2",
    "tslint": "^5.8.0",
    "typescript": "^2.6.2"
  },
  "keywords": [
    "either",
    "maybe",
    "tuple",
    "identity",
    "functional",
    "fp",
    "monad",
    "adt",
    "validation"
  ],
  "files": [
    "src",
    "lib",
    "index.flow.js",
    "es",
    ".flowconfig"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zerobias/apropos.git"
  },
  "bugs": {
    "url": "https://github.com/zerobias/apropos/issues"
  },
  "workspaces": [
    "packages/*"
  ]
}
